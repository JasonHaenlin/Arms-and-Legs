        <pattern>lsl r(?&lt;1&gt;[0-9]+), r(?&lt;2&gt;[0-9]+), #(?&lt;3&gt;[0-9]+)</pattern>
        <ranges>
            <range max="15" min="13" value="0">
            </range>
            <range max="12" min="11" value="0">
            </range>
            <range max="10" min="6" value="#3">
            </range>
            <range max="5" min="3" value="#2">
            </range>
            <range max="2" min="0" value="#1">
            </range>
        </ranges>
    </command>
